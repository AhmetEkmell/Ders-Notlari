#include <step_motor.h>

#include <lcd.c>

void saga(int16);
void sola(int16);


void main()
{
      int8 x;
      lcd_init();

     while(TRUE)
     {
         
   
      
     }

}

void saga(int16 hiz)
{
   output_a(0b00001010);
   delay_ms(hiz);
   output_a(0b00000110);
   delay_ms(hiz);
   output_a(0b00000101);
   delay_ms(hiz);
   output_a(0b00001001);
   delay_ms(hiz);
   output_a(0); // bobinlerde 1 0 kalmasý engellendi.

}

void sola(int16 hiz)
{
   output_a(0b00001001);
   delay_ms(hiz);
   output_a(0b00000101);
   delay_ms(hiz);
   output_a(0b00000110);
   delay_ms(hiz);
   output_a(0b00001010);
   delay_ms(hiz);
   output_a(0); // bobinlerde 1 0 kalmasý engellendi.

}

