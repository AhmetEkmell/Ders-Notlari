#include <yuruyen_led.h>

void main()
{

   while(TRUE)
   {
      output_c(1);  // toggle ile eger portC5 0 ise 1, 1 ise 0 yapiyor.
      delay_ms(250);
      output_c(0);
      delay_ms(250); // 250ms gecikme uygulandý.
      output_c(1);  // toggle ile eger portC5 0 ise 1, 1 ise 0 yapiyor.
      delay_ms(250);
      output_c(0);
      delay_ms(250); // 250ms gecikme uygulandý.
      output_c(1);  // toggle ile eger portC5 0 ise 1, 1 ise 0 yapiyor.
      delay_ms(250);
      output_c(0);
      delay_ms(250); // 250ms gecikme uygulandý.
   }

}
